<html>
    <head>
        
        <title>Weather</title>
        
        <link rel="icon" href="/img/weather.png">
       <link rel="stylesheet" type="text/css" href="/css/style.css">
     
    </head>
    <body>
        <div class="main-content">
            {{>header}}
           <p>Search for weather here!!</p>
            <form>
                <div class="container">
                <img src="/img/myLoc.png"  onclick="getUserLocation()" class="myLoc">
                <input type="text"  placeholder="Enter Location.." class="form-1" id="WeatherInput" >
                 </div>
                <button>Search</button>
             
            </form>
            <p id="message"> </p>
            <p id="message-1" ></p>
            <p id="message-2"></p>

        </div>
        <div id="map"></div>
      
    
        {{>footer}}

            
           <script src="/js/app.js"></script>
        
    </body>
</html>